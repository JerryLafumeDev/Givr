                                    </ul>
                                </div>
                            </div>
                            <div class="product-info">
                                <div class="product-tag">
                                    <i class="fas fa-tags"></i>
                                    <ol class="breadcrumb">
                                        <li class="breadcrumb-item"><a href="#"><%= posts[i].category%></a></li>
                                        <li class="breadcrumb-item active" aria-current="page"><%= posts[i].tag%></li>
                                    </ol>
                                                                        <ul class="product-widget">
                                            <li>
                                                <a href="compare.html" class="tooltip">
                                                    <i class="far fa-envelope"></i>
                                                    <span class="tooltext top">Ask Questions</span>
                                                </a>
                                            </li>
                                            <li>
                                                <button class="tooltip">
                                                    <i class="far fa-bookmark"></i>
                                                    <span class="tooltext top">bookmark</span>
                                                </button>
                                            </li>
                                        </ul>
                                </div>
                                <div class="product-title">
                                    <h5><a href="/grid-details/<%= posts[i]._id%>" class="realTitle" id="<%= posts[i].category%><%= posts[i]._id%>"><%= posts[i].title%></a></h5>
                                    <script> 
                                        console.log('Client-side code running');
                                        
                                        const button<%= posts[i]._id%> = document.getElementById('<%= posts[i].category%><%= posts[i]._id%>');
                                        button<%= posts[i]._id%>.addEventListener('click', function(e) {
                                        console.log('<%= posts[i].title%> was clicked');
                                        const title = document.getElementById('<%= posts[i].category%><%= posts[i]._id%>').innerText
                                        const clicks = document.getElementById('clicks<%= posts[i]._id%>').innerText

                                        fetch('/clicked', {method: 'POST', headers: {'Content-Type': 'application/json'}, body: JSON.stringify({'title': title, 'clicks': clicks})})
                                        
                                            .then(function(response) {
                                            if(response.ok) {
                                                console.log('click was recorded');
                                                return;
                                            }
                                            throw new Error('Request failed.');
                                            })
                                            .catch(function(error) {
                                            console.log(error);
                                            });
                                        })
                                        
                                        setInterval(function() {
                                          fetch('/clicks', {method: 'GET'})
                                            .then(function(response) {
                                              if(response.ok) return response.json();
                                              throw new Error('Request failed.');
                                            })
                                            .catch(function(error) {
                                              console.log(error);
                                            });
                                        }, 1000000);</script>
                                    <ul class="product-location">
                                        <li><i class="fas fa-map-marker-alt"></i><p style="font-size: 12px;"><%= posts[i].city%>, <%= posts[i].state%></p></li>
                                        <li style="margin-left: auto; margin-right: 0 !important;"><i class="fas fa-clock"></i><p style="font-size: 11px;"><%= moment(posts[i].date).fromNow() %></p></li>
                                    </ul>
                                </div>
                                <div class="product-details">
                                    <div class="product-price">
                                        <h5>Free</h5>
                                        <span>/Negotiable</span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
